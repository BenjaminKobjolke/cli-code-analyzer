{
  "log_level": "all",
  "max_lines_per_file": {
    "enabled": true,
    "warning": 200,
    "error": 300
  },
  "pmd_duplicates": {
    "enabled": true,
    "minimum_tokens": 100,
    "exclude_patterns": {
      "dart": ["*.g.dart", "*.freezed.dart"]
    }
  },
  "dart_analyze": {
    "enabled": true
  },
  "dart_code_linter": {
    "enabled": false
  },
  "flutter_analyze": {
    "enabled": false
  }
}
